        "# ver en un grafico los rangos de edades, las categorias y cuantas personas hay\n",
        "bins = [0, 12, 18, 30, 45, 60, 80]\n",
        "labels = ['Niños', 'Adolescentes', 'Jóvenes', 'Adultos', 'Adultos medios', 'Adultos mayores']\n",
        "rangos = [\"0-12\", \"13-18\", \"19-30\", \"31-45\", \"46-60\", \"61-80\"]\n",
        "\n",
        "df['grupo_edad'] = pd.cut(df['age'], bins=bins, labels=labels, include_lowest=True)\n",
        "\n",
        "conteo = df['grupo_edad'].value_counts().sort_index()\n",
        "\n",
        "etiquetas = [f\"({rangos[i]} {labels[i]} = {conteo.iloc[i]})\" for i in range(len(conteo))]\n",
        "\n",
        "plt.figure(figsize=(10,5))\n",
